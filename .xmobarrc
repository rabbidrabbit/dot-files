Config { font = "-*-Terminus-*-20-*-*-*-*-*-*-*"
       , bgColor = "#2d2d2d"
       , fgColor = "#808080"
       , position = TopW L 100
       ,lowerOnStart = True
       , commands = [ Run Weather "EGPF" ["-t"," <tempF>F","-L","64","-H","77","--normal","green","--high","red","--low","lightblue"] 36000
                    , commands = [ Run Cpu ["-L","3","-H","50","--normal","green","--high","red"] 10
       	      , Run Battery ["-t", "Batt:<watts> <left>%", "-H", "75", "-L", "15", "--low", "red", "--high", "green"] 600
                    , Run Memory ["-t","Mem: <usedratio>%"] 10
                    , Run Swap [] 10
                    , Run Date "%a %b %_d %l:%M" "date" 10
                    , Run Com "uname" ["-n", "-s"] "" 36000
                    , Run Com "$HOME/git/x-config/music.sh" [""]  "music" 30
                    , Run StdinReader
                    ]
       , sepChar = "%"
       , alignSep = "}{"
       , template = "%StdinReader% }{ <fc=#4d4d4d>%music%</fc> | %cpu% | %memory% * %swap% | %uname% | <fc=#ee9a00>%date%</fc> | %thermal0% | %battery%"
       }
